{
  "manifest_version": 3,
  "name": "Github Todo",
  "version": "1.0",
  "description": "List the things you need to do on Github.",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": ["sidePanel"]
}
